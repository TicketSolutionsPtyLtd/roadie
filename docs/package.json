{
  "name": "docs",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "prepare": "panda codegen",
    "build": "next build",
    "clean": "rm -rf node_modules roadie-core .next out",
    "dev": "run-p dev:*",
    "dev:panda": "panda -w",
    "dev:next": "next dev",
    "start": "next start",
    "format": "prettier --ignore-path ../.prettierignore --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --ignore-path ../.prettierignore --check \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@mdx-js/loader": "3.1.1",
    "@mdx-js/react": "3.1.1",
    "@next/mdx": "16.0.1",
    "@oztix/roadie-components": "workspace:*",
    "@oztix/roadie-core": "workspace:*",
    "@types/mdx": "2.0.13",
    "lucide-react": "0.548.0",
    "next": "16.0.1",
    "next-view-transitions": "0.3.4",
    "prism-react-renderer": "2.4.1",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-live": "4.1.8",
    "remark-gfm": "4.0.1"
  },
  "devDependencies": {
    "@pandacss/dev": "1.4.3",
    "@types/node": "24.9.2",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "eslint": "9.38.0",
    "eslint-config-next": "16.0.1",
    "eslint-plugin-mdx": "3.6.2",
    "npm-run-all": "4.1.5",
    "react-docgen-typescript": "2.4.0",
    "unist-util-visit": "5.0.0"
  }
}
