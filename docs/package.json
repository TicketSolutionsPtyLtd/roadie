{
  "name": "docs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "panda codegen",
    "build": "next build",
    "clean": "rm -rf node_modules roadie-core .next",
    "dev": "run-p dev:*",
    "dev:panda": "panda -w",
    "dev:next": "next dev",
    "start": "next start",
    "format": "prettier --ignore-path ../.prettierignore --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --ignore-path ../.prettierignore --check \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "lint": "eslint --ignore-path ../.eslintignore . --ext .ts,.tsx",
    "lint:fix": "eslint --ignore-path ../.eslintignore . --ext .ts,.tsx --fix"
  },
  "dependencies": {
    "@mdx-js/loader": "^3.1.0",
    "@mdx-js/react": "^3.1.0",
    "@next/mdx": "^15.1.0",
    "@oztix/roadie-components": "workspace:*",
    "@oztix/roadie-core": "workspace:*",
    "@types/mdx": "^2.0.13",
    "lucide-react": "^0.468.0",
    "next": "15.0.3",
    "next-view-transitions": "^0.3.4",
    "prism-react-renderer": "^2.4.0",
    "react": "^19.0.0",
    "react-aria": "^3.36.0",
    "react-dom": "^19.0.0",
    "react-live": "^4.1.8",
    "remark-gfm": "^4.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@pandacss/dev": "^0.48.1",
    "@types/node": "^20.17.9",
    "eslint": "^8.57.1",
    "eslint-config-next": "15.0.3",
    "eslint-plugin-mdx": "^3.1.5",
    "npm-run-all": "^4.1.5",
    "react-docgen-typescript": "^2.2.2",
    "typescript": "^5.7.2"
  }
}
